@model InternetServicesProvider.Entities.BookedPlan
@{
    ViewData["Title"] = "Book Plan Details";
}
<div class="card" style="width:100%; margin-top:10px;">
    <div class="card-header bg-dark text-white">
        <h5>Booked Internet Services Plan Details</h5>
    </div>
    <div class="card-body">
        <table class="table table-borderless">
            <tr>
                <td>
                    <b>@Html.DisplayNameFor(m => m.UserName)</b>
                </td>
                <td>@Html.DisplayFor(m => m.UserName)</td>
                <td>
                    <b>@Html.DisplayNameFor(m => m.CustomerId)</b>
                </td>
                <td>@Html.DisplayFor(m => m.CustomerId)</td>
            </tr>
            <tr>
                <td>
                    <b>@Html.DisplayNameFor(m => m.PlanName)</b>
                </td>
                <td>@Html.DisplayFor(m => m.PlanName)</td>
                <td>
                    <b>@Html.DisplayNameFor(m => m.Email)</b>
                </td>
                <td>@Html.DisplayFor(m => m.Email)</td>
            </tr>
            <tr>
                <td>
                    <b>@Html.DisplayNameFor(m => m.Address)</b>
                </td>
                <td>@Html.DisplayFor(m => m.Address)</td>
            </tr>
        </table>
        <p>
            @Html.ActionLink("Back to Home", "Index", "Home", new { @class = "btn btn-primary" })
        </p>
    </div>
</div>